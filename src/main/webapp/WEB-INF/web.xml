<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="4.0"
         xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
   http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <display-name>Archetype Created Web Application</display-name>


    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>

    <servlet>
        <servlet-name>HomeworkPartAServlet</servlet-name>
        <servlet-class>ru.mail.ilya6697089.app.controller.HomeworkPartAServlet</servlet-class>
        <load-on-startup>0</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>HomeworkPartAServlet</servlet-name>
        <url-pattern>/HomeworkPartA</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>HomeworkPartBServlet</servlet-name>
        <servlet-class>ru.mail.ilya6697089.app.controller.HomeworkPartBServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>HomeworkPartBServlet</servlet-name>
        <url-pattern>/HomeworkPartB</url-pattern>
    </servlet-mapping>


    <servlet-mapping>
        <servlet-name>default</servlet-name>
        <url-pattern>/</url-pattern>
    </servlet-mapping>

</web-app>
